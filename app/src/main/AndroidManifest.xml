<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.how_not_to_get_divorced">

    <application
        android:name=".Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_bell"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_bell_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Hownottogetdivorced">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Hownottogetdivorced.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name=".receivers.ContinuousAlarmReceiver"/>
        <receiver android:name=".receivers.DiscreteAlarmReceiver"/>
    </application>

</manifest>